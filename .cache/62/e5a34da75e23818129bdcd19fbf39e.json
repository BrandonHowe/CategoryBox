.ng/geom-poly-utils\";\nimport { PI, TAU } from \"@thi.ng/math\";\nimport { dist } from \"@thi.ng/vectors\";\nimport { dispatch } from \"../internal/dispatch\";\n/**\n * Returns the arc length / perimeter / circumference of the given\n * shape. For groups calls {@link arcLength} for each child and returns the\n * sum of results.\n *\n * Implemented for:\n *\n * - Circle\n * - Ellipse\n * - Group\n * - Line\n * - Polygon\n * - Polyline\n * - Quad\n * - Rect\n * - Triangle\n *\n */\nexport const arcLength = defmulti(dispatch);\narcLength.addAll({\n    [Type.CIRCLE]: ($) => TAU * $.r,\n    [Type.ELLIPSE]: ({ r: [a, b] }) => \n    // Ramanujan approximation\n    // https://www.mathsisfun.com/geometry/ellipse-perimeter.html\n    PI * (3 * (a + b) - Math.sqrt((3 * a + b) * (3 * b + a))),\n    [Type.GROUP]: ({ children }) => children.reduce((sum, $) => sum + arcLength($), 0),\n    [Type.LINE]: ({ points }) => dist(points[0], points[1]),\n    [Type.POLYGON]: ({ points }) => perimeter(points, points.length, true),\n    [Type.POLYLINE]: ({ points }) => perimeter(points, points.length),\n    [Type.RECT]: ({ size }) => 2 * (size[0] + size[1]),\n    [Type.TRIANGLE]: ({ points }) => dist(points[0], points[1]) +\n        dist(points[1], points[2]) +\n        dist(points[2], points[0]),\n});\narcLength.isa(Type.QUAD, Type.POLYGON);\narcLength.isa(Type.TRIANGLE, Type.POLYGON);\n"},"lineCount":null}},"error":null,"hash":"84f9f9ae205a01d53a1b5dae78bb71ea","cacheData":{"env":{}}}